PREFIX ex: <http://example.org/>
PREFIX wdt: <http://www.wikidata.org/prop/direct/>
PREFIX wd: <http://www.wikidata.org/entity/>

SELECT (COUNT(?seats) AS ?numberOfSeats) WHERE {
  ?team wdt:P31 wd:Q33739;  # instance of football club
        wdt:P115 ?stadium.  # home venue
  ?stadium wdt:P2048 ?seats. # seating capacity
  FILTER(?team = wd:Q100)   # FC Porto
}