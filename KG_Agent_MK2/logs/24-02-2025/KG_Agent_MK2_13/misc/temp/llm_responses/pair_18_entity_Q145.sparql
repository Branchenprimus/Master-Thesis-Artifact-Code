PREFIX wdt: <http://www.wikidata.org/prop/direct/>
PREFIX wd: <http://www.wikidata.org/entity/>

SELECT ?monarch WHERE {
  ?monarch wdt:P31 wd:Q116;  # instance of monarch
           wdt:P39 ?position.  # position held
  ?position wdt:P17 wd:Q145;   # position held in United Kingdom
  ?monarch wdt:P26 ?spouse.    # married to
  ?spouse wdt:P31 wd:Q188.     # spouse is German
}